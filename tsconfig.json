{
  "version": "1.8.2",
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "target": "ES5",
    "sourceMap": true
  },
  "filesGlob": [
    "mocha.conf.ts",
    "typings/tsd.d.ts",
    "client/**/*.ts"
